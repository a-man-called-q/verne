{
  "name": "{{pkgName}}",
  "version": "0.0.0",
  "private": true,
  {{#if isBuildable}}
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  {{/if}}
  "scripts": {
    {{#if isBuildable}}
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit"
    {{else}}
    "lint": "biome check ."
    {{/if}}
  }{{#if isBuildable}},
  "devDependencies": {
    "unbuild": "catalog:package",
    "typescript": "catalog:common",
  }
  {{/if}}
}
