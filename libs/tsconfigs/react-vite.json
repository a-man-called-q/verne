{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "useDefineForClassFields": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  }
}
